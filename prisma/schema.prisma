generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model customer {
  customer_id   String  @id
  name          String
  gender        String
  email         String
  phone_number  String
  device        String
  menu          String
  location      String
  createAt      String
} 
